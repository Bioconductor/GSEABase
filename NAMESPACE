import(methods)

importClassesFrom(Biobase, ScalarCharacter, Versions)

exportClasses(GeneIdentifierType,
              NullIdentifier, AnnotationIdentifier, EntrezIdentifier,
              SymbolIdentifier, PfamIdentifier,
              CollectionType,
              AdHocCollection, BroadCollection,
              GeneSet, GeneColorSet)

exportMethods(show, initialize, coerce,
              validIdentifiers, annotation,
              ## GeneSet
              GeneSet,
              
              setType, genes, setIdentifier, setName, description,
              longDescription, organism, pubMedIds, urls, contributor,
              setVersion, creationDate, collectionType,

              "setType<-", "genes<-", "setIdentifier<-", "setName<-",
              "description<-", "longDescription<-", "organism<-",
              "pubMedIds<-", "urls<-", "contributor<-",
              "setVersion<-", "creationDate<-", "collectionType<-",

              intersect, union, setdiff, "&", "|",

              ## GeneColorSet
              GeneColorSet, phenotype, geneColor, phenotypeColor)

export(getBroadSets, toBroadXML)
